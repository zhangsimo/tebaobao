page{
  height: 100%;
}

.container{
  background:#f8f8f8;
  color:#333;
  height: 100%;
}

.header{
  height: 256rpx;
  background:#ff4965;
  background-size: 100% 256rpx;
  background-repeat: no-repeat;
  background-position: 50%;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.user-box{
  width:100%;
  padding:0 48rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  align-items: center;
  color:#666;
}

.header .avatar-wrap{
  width:120rpx;
  height: 120rpx;
  border-radius: 50%;
  box-shadow: 0px 0px 5px #333333;
  margin:0 auto;
}

.header .avatar-wrap .avatar{
  width:120rpx;
  height:120rpx;
  border-radius: 50%;
  margin:0 auto;
}
.header .user-info{
  font-size: 12px;
  width:100%;
  padding-left:48rpx;

}

.user-name {
  font-size: 32rpx;
  color:#fff;
  display: inline-block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.user-login {
  display: inline-block;
  padding: 0 8px;
  font-size: 30rpx;
  border: 1px solid #fff;
  color: #fff;
}

.user-box .user-points {
    display: block;
    border-left: 1px solid #fff;
    padding: 0 0 0 16px;
    color: #666;
    position: relative;
    white-space: nowrap;
}

.user-box .user-points::before {
    content: "";
    display: block;
    border-left: 1px solid #bcc0cd;
    position: absolute;
    left: 0;
    top: 0;
    width: 1px;
    height: 100%;
}

.user-points-text{
  font-size: 50rpx;
}

.orders-type{
  display: flex;
  flex-direction: row;
  align-items: center;
  background:#fff;
  width:100%;
  padding: 50rpx 0 30rpx;
}

.flex-item5, .flex-item4{
  flex:5;
  text-align: center;
  border-left: 1px solid #f2f2f2;
  padding: 20px 0;
  position: relative;
  color:#666;
}

.flex-item4{
  flex:4;
}

.orders-type navigator:first-child{
  border-left:none;
}

.orders-type navigator:last-child::before {
    content: "";
    display: block;
    width: 6px;
    height: 6px;
    position: absolute;
    left: -4px;
    top: 50%;
    margin-top: -2px;
    border-left: 1px solid #f2f2f2;
    border-bottom: 1px solid #f2f2f2;
    transform: rotate(-135deg);
    -webkit-transform: rotate(-135deg);
    -moz-transform: rotate(-135deg);
    background: #fff;
}

.flex-item5 .ico,.flex-item4 .ico{
  width: 25px;
  height: 25px;
  margin: 0 auto;
  display: block;
  background-position: 50%;
  background-repeat: no-repeat;
  background-size: 25px 25px;
  text-align: center;
  position: relative;
}

